<section class="meetings-page" id="meetings">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="row">
            <div class="col-lg-12">
              <div class="filters">
                <ul>
                  <li data-filter="*"  class="active">All Meetings</li>
                  <li data-filter=".soon">Soon</li>
                  <li data-filter=".imp">Important</li>
                  <li data-filter=".att">Attractive</li>
                </ul>
              </div>
            </div>
            <div class="col-lg-12">
              <div class="row grid">

{% for row in rows %}
            <div class="col-lg-4 templatemo-item-col all att soon">
                  <div class="meeting-item">
                    <div class="thumb">
                      <div class="price">

                        <span>${{ row['content']['#row']._entity.field_price.value}} </span>
                      </div>

                      <a href="http://localhost/web/themeexc/web/node/{{ row['content']['#row']._entity.id }}"><img src="themes/custom/azure/images/meeting-01.jpg" alt=""></a>
                    </div>
                    <div class="down-content">
                      <div class="date">
                        <h6>Nov <span>{{ row['content']['#row']._entity.field_price.value}} </span></h6>
                      </div>
                      <a href="http://localhost/web/themeexc/web/node/{{ row['content']['#row']._entity.id }}"><h4>{{ row['content']['#row']._entity.field_meeting_title.value}}</h4></a>
                     {{ row['content']['#row']._entity.body.value | raw }} 
                    </div>
            </div>
</div>

{% endfor %}

 </div>
            </div>
            <div class="col-lg-12">
              <div class="pagination">
                <ul>
                  <li><a href="#">1</a></li>
                  <li class="active"><a href="#">2</a></li>
                  <li><a href="#">3</a></li>
                  <li><a href="#"><i class="fa fa-angle-right"></i></a></li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>



